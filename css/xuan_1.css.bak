.container{
margin:0px auto;
position:relative;
width:1000px;
font-size:.8125em;
}
header{
height:72px;
overflow:hidden;
background:url(../pictures/header-bg.gif) 0 0 repeat-x;
}
h1{
font-size:34px;
line-height:1.2em;
color:#fff;
font-weight:normal;
background:url(../pictures/logo.jpg) 0 0 no-repeat;
position:absolute;
padding:6px 0 8px 70px;
left:39px;
top:-18px;
}
h1 a{
color:#fff;
text-decoration:none;
}
a{
color:#f27f02;
outline:none;
}
a:hover{
text-decoration:none;
}
header nav{
float:right;
display:block;
}
header nav ul li{
float:left;
list-style:none;
}
header nav ul li a{
font-size:22px;
color:#fff;
height:67px;
line-height:67px;
text-decoration:none;
width:101px;
text-align:center;
float:left;
background:#433b8f;
margin-top:-12px;
}
header nav ul li a:hover{
padding-bottom:5px;
}
header nav ul li:nth-of-type(2) a {
background:#0184cd;
}
header nav ul li:nth-of-type(3) a {
background:#7cbc19;
}
header nav ul li:nth-of-type(4) a {
background:#ffbc00;
}
section{
height:496px;
overflow:hidden;
background:url(../pictures/gallery-bg.gif) 0 0 repeat-x;
}
section div ul li{
list-style-type:none;
}
/*===== gallery =====*/
.roundabout-holder  { 
	width:850px;
	height:496px;
	margin:0 auto;
}
.roundabout-moveable-item {
   width: 646px;
   height: 416px;
   cursor: pointer;
	 border:3px solid #ccc;
	 border:3px solid rgba(0, 0, 0, 0.08);
   border-radius:4px;
	 -moz-border-radius:4px;
	 -webkit-border-radius:4px;
}
.roundabout-moveable-item img{
		width:100%;
}
.roundabout-in-focus {
	border:3px solid rgba(0, 0, 0, 0.2);
}
.myContainer{
width:1000px;
margin:0 auto;
position:relative;
text-align:center;
}
.nav_control{
list-style-type:none;
}
h2{
font-size:28px;
}
h2 span{
color:#999;
}
.changeImg{
border:2px solid rgba(0,0,0,0.5);
}
.nav_control li{
float:left;
padding-right:200px;
margin-left:180px;
margin-bottom:100px;
}
.menu_list{
float:right;
background:url(../pictures/header-bg.gif) 0 0 repeat-x;
position:relative;
width:100%;
}
.menu_list ul{
list-style-type:none;
position:relative;
}
.menu_list ul li{
float:left;
width:70px;
height:70px;
line-height:70px;
border-right:1px solid #f00;
margin-top:-30px;
padding:0px 20px;
text-align:center;
}
.menu_container{
width:1000px;
font-size:2em;
text-align:center;
color:#fff;
margin-left:300px;
}
.menu_list ul li:hover{
color:green;
cursor:pointer;
}
.drop_theme{
display:none;
position:absolute;
background-color:#f9f9f9;
min-width:80px;
box-shadow:0px 8px 16px 0px rgba{0,0,0,0.2};
}
.drop_theme a{
font-size:20px;
}
.drop_text{
display:none;
position:absolute;
background-color:#f9f9f9;
min-width:80px;
box-shadow:0px 8px 16px 0px rgba{0,0,0,0.2};
}
.drop_nice{
display:none;
position:absolute;
background-color:#f9f9f9;
min-width:80px;
box-shadow:0px 8px 16px 0px rgba{0,0,0,0.2};
}
.li_text:hover{
}
.li_theme:hover{
display:block;
}
.li_nice:hover{
}